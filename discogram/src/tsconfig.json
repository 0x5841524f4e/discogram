
{
	"compilerOptions": {
		"outDir": "../dist",
		"rootDir": ".",
		"baseUrl": ".",
		"target": "es6",
		"module": "commonjs",
		"strict": false,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"composite": true,
		"declaration": true,
		"sourceMap": true,
		"inlineSources": true,
		"removeComments": true
	}
}